{
  "name": "notion-exporter",
  "author": "Yann Bolliger",
  "repository": "github.com/yannbolliger/notion-exporter",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "module": "dist/notion-exporter.esm.js",
  "bin": "./bin/index.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint src",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "axios": "^0.21.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/adm-zip": "^0.4.33",
    "@types/uuid": "^8.3.0",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-jsdoc": "^0.3.18",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src"
    }
  },
  "size-limit": [
    {
      "path": "dist/notion-exporter.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/notion-exporter.esm.js",
      "limit": "10 KB"
    }
  ]
}
